pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout your source code from your version control system (e.g., Git)
                // For Git:
                git 'https://github.com/shivaayrawat/sonartest.git'
            }
        }

        stage('SonarQube Analysis') {
                 environment { Sonarurl= "http://20.219.236.142:9010"}
            steps {
                withCredentials([string(credentialsId: 'sonarqube', Variable: 'tetsing for demo')]) {
                    sh 'mvn sonar:sonar -Dsonar.login=$tesing for demo -Donar.host.url=${Sonarurl}'
            }
        }
    }
 }
}


